web:
    port: 8080
    read_timeout: 10s
    write_timeout: 10s
    idle_timeout: 2m0s
queue:
    workers: 2
    buffer_size: 120
    retry:
        max_retries: 10
        base_delay: 5s
    circuit_breaker:
        failure_threshold: 5
        timeout: 1m0s
sensor:
    type: simulated
    read_interval: 10s
    bme280:
        i2c_address: 118
        i2c_bus: ""
    simulation:
        min_temperature: 15
        max_temperature: 35
        min_humidity: 30
        max_humidity: 80
        min_pressure: 98000
        max_pressure: 102000
openweather:
    timeout: 30s
timeouts:
    shutdown_timeout: 10s
    queue_shutdown_timeout: 30s
    web_shutdown_timeout: 30s
    processing_timeout: 5s
